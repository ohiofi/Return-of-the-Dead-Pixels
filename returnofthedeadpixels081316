======
RULES
======

[dp3]->restart
[dp2]->[dp3] Sfx5
[deadplayer]->[dp2] Sfx5 again
[|Player|Enemy] -> [blood4|DeadPlayer|Enemy Sfx1] again
late [Player|Tools] -> [Player|Floor Sfx4] checkpoint
late [Player|firstAidKit] -> [Player|Floor Sfx4] checkpoint
late [Player chckpnt] -> [player chckpnt] checkpoint
[ >  Player | Car01 ] -> [  >  Player | > Car01  ]
[ >  Player | chair ] -> [  >  Player | > chair floor ]
[ >  Player | table ] -> [  >  Player | > table floor ]
[ >  Player | Crate ] -> [  >  Player | > Crate  ]
[ stationary zombie ] -> [randomDir zombie]
[ Player Blood1 ] -> [ Player Blood3 Sfx0]
[ Player Blood2 ] -> [ Player Blood3 Sfx3]
[ Player Grass ] -> [Player Sfx2]
[Player|...|BadGuy] -> [Player|...| < BadGuy]
vertical [|Blood4|]->[blood5|Blood4|blood5]
horizontal [|Blood4|]->[blood5|Blood5|blood5]
[> Zombie|Girl]->[< Zombie|Girl]
[Zombie|Target]->[< Zombie|Target]
[fire|Target]->[< fire|Target]
late [fire|fireproof]->[burntfloor|fireproof]
[|fire Target]->[fire|Target]
late[fire|player|fire] -> [fire|deadplayer|fire] Sfx1 again
late[fire|player|player|fire] -> [fire|deadplayer|deadplayer|fire] Sfx1 again
[stationary fire3 ] -> [randomDir fire1]
[stationary fire2 ] -> [randomDir fire3]
[stationary fire1 ] -> [randomDir fire2]
random [fire3|walls] -> [fire1|fire1 burntfloor]
random [fire3|walls] -> [fire1|fire1 burntfloor2]
random [fire3|enemy] -> [fire1|fire1 burntfloor2]Sfx6
random [fire1] -> [fire2]
random [fire2] -> [fire3]
random [fire3] -> [fire1]
random [fire3|floor no player] -> [burntfloor|fire2] Sfx7
random [fire3|floor no player] -> [burntfloor2|fire2] Sfx7
random [fire3|tree] -> [burntfloor2|fire2] Sfx7
[fire3|grass] -> [fire2 burntfloor|fire1]
[fire3|Car01] -> [burntfloor2|fire1]
[fire3|Crate] -> [burntfloor2|fire1]
[fire|fire|fire] -> [fire1|burntfloor|fire1]
[fire|cars]->[fire|burntcar]
[firestarter|player]->[fire1|player]
[firestarter|fire]->[fire1|fire1]
[water5] -> [water1]
[water4] -> [water5]
[water3] -> [water4]
[water2] -> [water3]
[water1] -> [water2]

[Player goal] -> [Player goal win] again
[Car01 GasStation] -> [Car01 GasStation win] again


